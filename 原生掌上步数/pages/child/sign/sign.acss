	background: #fdc91d;
	border-radius: 35rpx;
	color: #FFFFFF;
	margin-top: 55rpx;
}
.close{
	margin-top: 80rpx;
}
.qd-modal{
	position: fixed;
	width: 100%;
	top: 220rpx;
	z-index: 999999;
}
.text-white{
  color:#fff!important
}
.tops{
  width: 750rpx;
height: 160rpx;
opacity: 1;
background: linear-gradient(90deg,#ff6782, #c3e1fc);
}
.signIn-heads{
	width: 100%;
	height: 420rpx;
	opacity: 1;
background: linear-gradient(90deg,#ff6782, #c3e1fc);
